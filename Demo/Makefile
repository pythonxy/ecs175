	-$(DEL_FILE) qrc_shaders.cpp
qrc_shaders.cpp: shaders.qrc \
		basic.frag \
		basic.vert
	/usr/lib/qt4/bin/rcc -name shaders shaders.qrc -o qrc_shaders.cpp

